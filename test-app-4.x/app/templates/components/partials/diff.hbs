<div class='example'>
  {{moment-diff this.lastHour this.currentTime}}
  <code>
    \{{moment-diff this.lastHour this.currentTime}}
  </code>
</div>

<div class='example'>
  {{moment-diff this.currentTime this.inTwelveHours precision='hour'}}
  <code>
    \{{moment-diff this.currentTime this.inTwelveHours precision='hour'}}
  </code>
</div>

<div class='example'>
  {{moment-diff this.currentTime this.inTwelveHours precision='day' float=true}}
  <code>
    {{! template linter bug: https://github.com/ember-template-lint/ember-template-lint/issues/2330 }}
    {{! template-lint-disable no-unbalanced-curlies}}
    \{{moment-diff this.currentTime this.inTwelveHours precision='day' float=true}}
  </code>
</div>

<div class='example'>
  {{moment-diff this.usIndependenceDay this.currentTime precision='year'}}
  <code>
    \{{moment-diff this.usIndependenceDay this.currentTime precision='year'}}
  </code>
</div>

<div class='example'>
  {{moment-diff this.currentTime this.usIndependenceDay precision='year'}}
  <code>
    \{{moment-diff this.currentTime this.usIndependenceDay precision='year'}}
  </code>
</div>